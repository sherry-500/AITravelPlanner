# 环境变量配置文件
# 复制此文件为 .env 并填入实际的 API 密钥

# OpenAI API 配置
VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_OPENAI_BASE_URL=https://api.openai.com/v1

# 百度语音 API 配置（可选）
VITE_BAIDU_APP_ID=your_baidu_app_id_here
VITE_BAIDU_API_KEY=your_baidu_api_key_here
VITE_BAIDU_SECRET_KEY=your_baidu_secret_key_here

# 高德地图 API 配置（可选）
VITE_AMAP_API_KEY=your_amap_api_key_here

# 应用配置
VITE_APP_NAME=AI旅行规划师
VITE_APP_VERSION=1.0.0